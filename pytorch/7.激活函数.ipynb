   "source": [
    "#### ReLU函数\n",
    "$$\n",
    "ReLU(x)=max(0,x)\n",
    "$$"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "import torch\n",
    "from torch import nn\n",
    "relu = nn.ReLU(inplace=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "tensor([[-1.4688,  1.1435,  0.3796],\n",
      "        [-0.3736,  0.0114, -0.1835]])\n",
      "tensor([[0.0000, 1.1435, 0.3796],\n",
      "        [0.0000, 0.0114, 0.0000]])\n",
      "tensor([[1, 1, 1],\n",
      "        [1, 1, 1]], dtype=torch.uint8)\n"
     ]
    }
   ],
   "source": [
    "input = torch.randn(2,3)\n",
    "print(input)\n",
    "output = relu(input)  # apply relu\n",
    "print(output)\n",
    "print(input == output) # 直接覆盖input"
   ]
  },
